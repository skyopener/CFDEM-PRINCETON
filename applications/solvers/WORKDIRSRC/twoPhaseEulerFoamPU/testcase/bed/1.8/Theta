FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "1.8";
    object      Theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            inletOutlet;
        phi             phi1;
        inletValue      uniform 1e-08;
        value           uniform 1e-08;
    }
    bottom
    {
        type            fixedValue;
        value           uniform 1e-08;
    }
    walls
    {
        type            zeroGradient;
    }
    frontBack
    {
        type            empty;
    }
}


