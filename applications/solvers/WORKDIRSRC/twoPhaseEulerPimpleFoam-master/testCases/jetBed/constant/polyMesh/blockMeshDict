FoamFile
{
    version         2.0; 
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
meshGenApp blockMesh;


convertToMeters 1.0;

vertices        
(
    (0      0       0    ) //  0
    (0.06   0       0    ) //  1
    (0.06   0       0.001) //  2
    (0      0       0.001) //  3
    (0      1       0    ) //  4
    (0.06   1       0    ) //  5
    (0.06   1       0.001) //  6
    (0      1       0.001) //  7
    (0.08   0       0    ) //  8
    (0.08   0       0.001) //  9
    (0.08   1       0    ) // 10
    (0.08   1       0.001) // 11
    (0.14   0       0    ) // 12
    (0.14   0       0.001) // 13
    (0.14   1       0    ) // 14
    (0.14   1       0.001) // 15
);

//          7 ---------- 6
//         /|           /|
//        / |          / |   z
//       3 ---------- 2  |   ^ 
//       |  |         |  |   |   y
//       |  4 --------|- 5   |  /
//       | /          | /    | /
//       |/           |/     |/
//       0 ---------- 1      -----> x

blocks          
(
    hex (0 1 5 4 3 2 6 7)       ( 12 100 1) simpleGrading (1 1 1)  // Left 
    hex (1 8 10 5 2 9 11 6)     ( 4  100 1) simpleGrading (1 1 1)  // Jet
    hex (8 12 14 10 9 13 15 11) ( 12 100 1) simpleGrading (1 1 1)  // Right
);


patches         
(
    patch    inlet
    (
        (0  1  2 3)
        (8 12 13 9)
    )

    patch    jet
    (
	(1 8 9 2)
    )

    patch    outlet
