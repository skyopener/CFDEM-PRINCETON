        (4   5  6  7)
        (5  10 11  6)
        (10 14 15 11)
    )

    wall     walls
    (
        (0   4  7  3)
        (13 12 14 15)
    )

    empty    frontAndBackPlanes
    (
        (0 1 5 4)
        (1 8 10 5)
        (8 12 14 10)
        (3 2 6 7)
        (2 9 11 6)
        (9 13 15 11)
    )
